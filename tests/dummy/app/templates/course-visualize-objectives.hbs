<section class="course-visualize-objectives">
  <div class="backtolink">
    <LinkTo @route="course-visualizations" @model={{this.model}}>
      {{t "general.otherVisualizations"}}
    </LinkTo>
  </div>
  <h2 class="clickable">
    <LinkTo @route="course" @model={{this.model}}>
      {{this.model.title}}
      {{this.model.academicYear}}
    </LinkTo>
  </h2>
  <h3>
    {{t "general.objectivesTotalInstructionalTime"}}
  </h3>
  <VisualizerCourseObjectives @course={{this.model}} />
</section>
